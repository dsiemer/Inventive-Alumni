extends layout

block content
    .mainFormContainer 
        .signInForm 
            .f-row.login-from 
                .f-column 
                    h2 Sign In 
                    form.new-user#new-user(action"" method="post") 
                        lable Email 
                        input.userInput#user-email(placeholder="Your Email" autofocus="autofocus" type="email") 
                        lable Password 
                        input.userInput#user-password(placeholder="Your Password" type="password") 
                        input(type="submit" name="commit" value="Sign In" data-disable-with="Sign In" class="signInbutton") 
    
block footer
